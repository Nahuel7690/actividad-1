<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        function Abundante(n){
            let numabundante = 0;
            for(let i; i < n; i++){
                numabundante += i;
            }
        }

        if(numabundante > n) return true;
        else return false;
    }
    const numero = prompt("digitar un numero: ");
    if(Abundante(numero)){
        document.write("el numero" + numero + "es abundante");
    }
    else{
        document.write("el numero "+ numero +"no es abundante " );
    }
    </script>
</body>
</html>
